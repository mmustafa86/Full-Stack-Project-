<%- include('header'); -%>

<body>

  <div>

    <h1 class="d-flex">result</h1>
  </div>

  <div class="container">
    <!-- Card -->
    <div class="row ">
      <!-- Card -->

      <%var b=[]%>
      <%for (var i=0 ;i < results.length; i++ ){%>
      <div class="card col-lg-3 col-sm-6 mb-4" style="width: 18rem;">
        <div class="card-body">

          <h5 id='apiName' class="card-title"><%= results[i].name%></h5>
          <!-- Text -->
          <%b.push(results[i].id)%>


          <p class="card-text"><%=results[i].description%></p>
          <!-- Link -->
          <a href="<%=results[i].url%>" class="btn btn-primary">Go somewhere</a>

          <form action="/add" method="POST">
            <input type="hidden" class="btn btn-info" name="nameId" value="<%=results[i].id%>" />
            <input type="hidden" class="btn btn-info" name="categoryId" value="<%=results[i].category%>" />


            <input id='dataName' type="hidden" value="<%=favorites%>">
           
<% if(favorites.sort().join(',')=== b.sort().join(',')){%>
  alert('same members');

  <%console.log('match')%>
<%}%>

            <button id="add" class="btn btn-info" type="submit">Add</button>


          </form>
        </div>
      </div>
      <%}%>
        
      
    </div>
      </div>
   






    </body>
<%- include('footer'); -%>

